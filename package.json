{
  "name": "figma-jb-variables-utilities",
  "version": "0.1.0",
  "private": true,
  "description": "Internal Figma plugin: tools launcher + token tools",
  "scripts": {
    "build": "node scripts/generate-custom-icons.cjs && build-figma-plugin --typecheck",
    "watch": "node scripts/generate-custom-icons.cjs --watch & build-figma-plugin --typecheck --watch",
    "preview": "vite",
    "preview:build": "vite build",
    "preview:serve": "vite preview",
    "test": "rm -rf .tmp-tests && tsc src/app/tools/print-color-usages/shared.ts --module commonjs --target es2020 --skipLibCheck --lib es2020 --types @figma/plugin-typings --outDir .tmp-tests/src && node --test tests/**/*.test.cjs",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "^4.0.3",
    "@create-figma-plugin/utilities": "^4.0.3",
    "preact": "^10.26.4"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^4.0.3",
    "@create-figma-plugin/tsconfig": "^4.0.3",
    "@preact/preset-vite": "^2.10.1",
    "@figma/plugin-typings": "*",
    "@types/node": "^22.13.10",
    "esbuild": "^0.25.3",
    "typescript": "^5.3.2",
    "vite": "^6.1.0"
  },
  "figma-plugin": {
    "id": "1598773743246786962",
    "name": "Int UI Design Tools",
    "editorType": [
      "figma"
    ],
    "networkAccess": {
      "allowedDomains": [
        "none"
      ]
    },
    "documentAccess": "dynamic-page",
    "permissions": [
      "teamlibrary"
    ],
    "menu": [
      {
        "name": "Tools Home",
        "main": "src/home/main.ts",
        "ui": "src/app/ui.tsx"
      },
      {
        "name": "Mockup Markup Quick Apply",
        "main": "src/mockup-markup-tool/main.ts",
        "ui": "src/app/ui.tsx"
      },
      {
        "name": "View Colors Chain",
        "main": "src/color-chain-tool/main.ts",
        "ui": "src/app/ui.tsx"
      },
      {
        "name": "Library Swap",
        "main": "src/library-swap-tool/main.ts",
        "ui": "src/app/ui.tsx"
      },
      {
        "name": "Print Color Usages",
        "main": "src/print-color-usages-tool/main.ts",
        "ui": "src/app/ui.tsx"
      },
      {
        "name": "Variables Export Import",
        "main": "src/variables-export-import-tool/main.ts",
        "ui": "src/app/ui.tsx"
      },
      {
        "name": "Rename Variables via JSON",
        "main": "src/variables-batch-rename-tool/main.ts",
        "ui": "src/app/ui.tsx"
      },
      {
        "name": "Variables Create Linked Colors",
        "main": "src/variables-create-linked-colors-tool/main.ts",
        "ui": "src/app/ui.tsx"
      },
      {
        "name": "Variables Replace Usages",
        "main": "src/variables-replace-usages-tool/main.ts",
        "ui": "src/app/ui.tsx"
      },
      {
        "name": "Find Color Match in Islands",
        "main": "src/find-color-match-tool/main.ts",
        "ui": "src/app/ui.tsx"
      }
    ]
  }
}
